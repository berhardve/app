{
  "routes": [
    {
      "src": "/raffle/raffles",
      "dest": "/index.js"
    },
    {
      "src": "/raffle/raffles/:raffleId/winners",
      "dest": "/index.js"
    },
    {
      "src": "/raffle/raffles/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "/raffle",
      "dest": "/raffle/index.html"
    }   
  ]
}
