{
    "routes": [
      {
        "src": "/raffles",
        "dest": "/index.js"
      },
      {
        "src": "/raffles/:raffleId/winners",
        "dest": "/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/raffle/index.html"
      }
      ,
      {
        "src": "/scripts.js",
        "dest": "/raffle/scripts.js"
      }
      ,
      {
        "src": "/style.css",
        "dest": "/raffle/scripts.js"
      }
    ]
  }
  